{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "resolveJsonModule":true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"]
    },
    "types": ["@nuxt/types", "@nuxtjs/axios", "@types/node"]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist",
    "src/static",
    "src/pages/jupswap.vue",
    "src/components/CoinSelectJup.vue",
    "src/components/CoinBlockJup.vue"
  ]
}
